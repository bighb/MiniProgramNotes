.page-content {
    width: 100%;
    padding-bottom: calc(48px + constant(safe-area-inset-bottom));
    padding-bottom: calc(48px + env(safe-area-inset-bottom));

}

.hyhb-bg {
    width: 100%;
    height: 484rpx;
}

.wrap {
    margin: 68rpx 76rpx;
}

.method-1-wrap {
    position: relative;
    border: 1rpx solid #e8e8e8;
    box-sizing: border-box;
    border-radius: 14rpx;
    padding: 30rpx 37rpx;
    color: #8b8b8b;
    font-size: 28rpx;
}

.method-1-wrap::before {
    content: "1";
    background: #b8d739;
    width: 44rpx;
    height: 44rpx;
    border-radius: 50%;
    position: absolute;
    top: -22rpx;
    left: -22rpx;
    color: #fff;
    z-index: 2;
    text-align: center;
    line-height: 44rpx;
}

