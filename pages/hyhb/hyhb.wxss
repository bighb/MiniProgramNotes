.title {
    position: absolute;
    top: -1em;
    left: 1%;
    line-height: 2em;
    padding: 0 1em;
    color: #333333;
    font-size: 36rpx;
    background-color: #fff;
}

.content {
    line-height: 50rpx;
}

.more-rules {
    height: 100rpx;
    text-align: center;
    color: #3f99fa;
    font-size: 24rpx;
    position: absolute;
    bottom: 0;
    width: calc(100% - 74rpx);
    background: linear-gradient(180deg, rgba(255, 255, 255, 0.00), #ffffff);
}

.map-warp {
    margin: 68rpx 0;
}

.head {
    position: relative;
    left: 1em;
    margin: 0 76rpx;

}

.head::before {
    content: "6";
    background: #b8d739;
    width: 44rpx;
    height: 44rpx;
    border-radius: 50%;
    position: absolute;
    top: 0;
    left: -50rpx;
    color: #fff;
    z-index: 2;
    text-align: center;
    line-height: 44rpx;
}

.title-info {
    color: #333333;
    font-size: 36rpx;
    padding-left: 13rpx;
}

.tip {
    color: #8b8b8b;
    font-size: 26rpx;
}

.map {
    width: 100%;
    height: 500rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    z-index: 9999;
}

ec-canvas {
    width: 100%;
    height: 100%;
}

.card {
    height: 410rpx;
    margin: 68rpx 76rpx 0 76rpx;
    border-radius: 14rpx;

}

.card image {
    width: 100%;
    height: 100%;
    border-radius: 14rpx;
}

.city-list {
    display: flex;
    flex-wrap: wrap;
    background-color: #f5f5f5;
    margin: 36rpx 76rpx;
    border-radius: 14rpx;
}

.city-list view {
    margin: 26rpx 12rpx 0rpx 12rpx;
    text-align: center;
    padding: 8rpx 20rpx;
}

.activate {
    background-color: #bfdb4d;
    color: #fff;
    border-radius: 30rpx;
    position: relative;
}

.activate::before {
    content: "";
    border: 14rpx solid transparent;
    border-bottom-color: #bfdb4d;
    position: absolute;
    top: -24rpx;
    left: 41%;
}

